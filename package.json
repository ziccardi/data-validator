{
  "name": "json-data-validator",
  "version": "3.0.1",
  "description": "Simple framework for dynamic json input validation",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "test": "jest --transformIgnorePatterns \"node_modules/(?!lodash-es)/\"",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "keywords": [
    "validator",
    "validation",
    "validate",
    "json",
    "configurable"
  ],
  "author": "Massimiliano Ziccardi <massimiliano.ziccardi@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/node": "16.11.35",
    "@types/is-valid-path": "0.1.0",
    "@types/lodash": "4.14.182",
    "@types/validator": "13.7.2",
    "gts": "3.1.0",
    "jest": "26.6.3",
    "ts-jest": "26.5.6",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "add": "2.0.6",
    "is-valid-path": "0.1.1",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "validator": "13.7.0",
    "yarn": "1.22.18"
  },
  "jest": {
    "preset": "ts-jest",
    "rootDir": "test",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ziccardi/json-data-validator.git"
  }
}